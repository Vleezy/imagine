    "@types/node": "20.5.8",
    "rimraf": "5.0.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "@rollup/plugin-terser": "0.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "~5.2.2"
  },
  "gitHead": "2305e183b7fa01b3837f0f20c401ecdec6858de6"
}